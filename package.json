{
  "name": "campus-market",
  "version": "1.0.0",
  "description": "Campus Market - Full Stack Application",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../frontend-react && npm install",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "cd frontend-react && npm start",
    "build": "npm run build:server && npm run build:frontend",
    "build:server": "cd server && npm run build",
    "build:frontend": "cd frontend-react && npm run build",
    "start": "cd server && npm start",
    "seed": "cd server && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gopinathsjsu/cmpe202-02-team-project-team_15.git"
  },
